@charset 'utf-8';
/* 轮播图主体样式定义 */
.yhd-content .menuwrap{height:400px;background:#F94B29;}
.menuwrap .mainbav{position:relative;overflow:hidden;width:1228px;height:100%;margin:auto;}
.menuwrap .mainbav .bgnav{width:5000px;}
.menuwrap .mainbav .bgnav > li{float:left;width:1228px;height:400px;background:url(../images/01-banner/0.jpg) repeat-x 205px;}
.menuwrap .mainbav .radiubtn{position:absolute; bottom:10px;left:50%;margin-left:-105px;}
.menuwrap .mainbav .radiubtn a{display:inline-block;width:12px;height:12px;margin-left:15px;

border-radius:50%;
background:rgba(255,255,255,0.5);
}
.menuwrap .mainbav .radiubtn a:hover{background:rgba(255,64,64,0.7);box-shadow:0 0 2px 2px #fff;}

.menuwrap .mainbav .midbtn span{position:absolute;top:50%;margin-top:-22px;width:44px;height:44px;
border-radius:50%;background:rgb(255,255,255);line-height:44px;opacity:0.5;text-align:center;font-size:20px;}
.menuwrap .mainbav .midbtn span.pre{left:215px;}
.menuwrap .mainbav .midbtn span.next{right:225px;}
.menuwrap .mainbav .midbtn span:hover{opacity:1;}
/* 
子菜单左导航样式定义 */
.menuwrap .mainbav .leftsidebar{
position:absolute;width:205px;height:100%;padding:3px 0px;background:rgba(0,0,0,0.7);}
.menuwrap .mainbav .leftsidebar .list{
height:32px;width:100%;margin-bottom:1px;line-height:32px;color:#fff;font-size:12px;text-indent:15px;}
.menuwrap .mainbav .leftsidebar .list i{margin-right:15px;font-size:18px;color:#656463;font-weight:bold;}

.menuwrap .mainbav .leftsidebar .list:hover{background:#fff;color:#000;}

.menuwrap .mainbav .leftsidebar .list:hover i{color:#FF6B44;}
  

/* 
二级菜单定义 */
.leftsidebar .subnav{display:none;position:absolute;top:0px;left:205px;z-index:999;width:1024px; 
    min-height:399px;background:#fff;margin:auto;padding-top:15px; border:1px solid #F2F2F2; 
   box-shadow:2px 2px 5px #666;}
.subnav .subnavcontent{/* width:707px; */padding:0px 20px;border-right:1px solid #F2F2F2;}
.subnav .subnavcontent dt{width:72px;padding:10px 0px;text-align:center;vertical-align:middle;font-size:14px;font-weight:bold;}
.subnavcontent dd{ width:647px;padding:10px 0px 0px 10px;border-bottom:1px solid #F2F2F2;word-spacing:15px;
  word-break:keep-all;
}
.subnavcontent dd > a{display:inline-block;padding-bottom:10px;}
.subnavcontent dd > a:hover{font-weight:bold;}

/* 子菜单右边样式 */
.subnav .subnavad {width:210px;padding:0px 20px;}
.subnav .subnavad li{margin:5px 0px;}
.subnav .subnavad li .ad{float:left; margin:10px 10px 10px 10px;  }
.subnav .subnavad li .ad img{width:80px;height:32px;}
.subnav .subnavad li > a > img{width:205px;height:110px;}

.menuwrap .mainbav .leftsidebar > li:hover .subnav{display:block;}


.mainbav .login{position:absolute;top:50%;right:20px;margin-top:-118px;width:205px;height:236px;
background:url(../images/01-banner/vip_bg2.png) no-repeat center/cover;}

.mainbav .login .loginlogo{position:absolute;left:50%;top:-30px;margin-left:-37px;z-index:2;
width:64px;height:64px;border-radius:50%;
background:url(../images/00-header/2.png) no-repeat 0px -150px;border:5px solid rgba(0,0,0,0.1);
}

.login p{margin-top:55px;text-align:center;}
.login .logbtn{margin:30px 0px 20px 0px;;text-align:center;}
.login .logbtn button{width:70px;height:26px;border-radius:20px;background-color:#fff;border:1px solid #FF4040;color:#FF4040;}
.login .logbtn button + button{margin-left:15px;}
.login .logad a{display:inline-block;padding:0px 0px 6px 20px;}
.login .logad a i{font-style:normal;}
.login .logad a:hover{color:#FF4040;}
.login .logbtn button:hover{background:#FF4040;color:#fff;}

/* 
网页主题内容整体居中样式 */

/* .yhd-content  */.autocontent{/* height:5000px; */width:1228px;margin:auto;/* border:1px solid red; */}
/* 一号抢购 *************************************************************************/
.autocontent .yhshopping{height:140px;margin-top:10px;}
/* 一号抢购左边 */
.yhshopping .shoppingbanner{position:relative;width:205px;height:100%;background:url(../images/00-header/2.png) repeat;text-align:center;}
.shoppingbanner h3{padding-top:45px;padding-bottom:5px;color:#000;font-weight:normal;}
.shoppingbanner span{display:inline-block;width:36px;height:27px;background:rgba(0,0,0,.8);color:#fff;font-size:18px;font-weight:bold;vertical-align:middle;}
.shoppingbanner > i{display:inline-block;height:27px;width:10px;vertical-align:middle;font-style:normal;font-weight:bold;font-size:18px;color:#000;}
.shoppingbanner h2{padding-top:5px;color:#fff;font-size:16px;font-weight:normal;}
.shoppingbanner > a{position:absolute;top:0px;right:0px;display:block;width:100%;height:100%;}
.shoppingbanner:hover h2{text-decoration:underline;}
/* 一号抢购右边 */
.yhshopping .shoppingcontainer{ position:relative;overflow:hidden;height:140px; width:1023px;}
.shoppingcontainer ul{width:5000px;}
.shoppingcontainer li{position:relative;width:225px;height:140px;padding:15px;background:#fff;}
.shoppingcontainer li:after{content:'';position:absolute;top:35px;right:0px;height:70px;width:1px;border-right:1px solid #ddd;}
.shoppingcontainer li h3.shoppingtext{overflow:hidden;width:100px;height:51px;margin-bottom:5px;font-weight:normal;}
.shoppingcontainer li img{ position:absolute;top:20px;right:10px;width: 110px;height: 110px;/*  width:125px;height:140px; */transition:0.5s;}
.shoppingcontainer li span{position:absolute;top:0px;right:0px;width:37px;height:37px;
background:url(../images/00-header/2.png) no-repeat -160px -150px;text-align:center;}
.shoppingcontainer li > a{position:absolute;top:0px;right:0px;display:block;width:100%;height:100%;}
.shoppingcontainer > a{position:absolute;top:50%;margin-top:-20px;width:20px;height:40px;background:#CCCCCC;
text-align:center;color:#000;line-height:40px;font-weight:bold;transition:0.5s;}
.shoppingcontainer .shoppiing_pre{right:-20px;}
.shoppingcontainer .shoppiing_next{left:-20px;}
.shoppingcontainer:hover .shoppiing_pre{right:0px;}
.shoppingcontainer li:hover img{right:15px;}



/* 一号闪购样式设计 ***************************************************************************/

.yhflashpurchase{/* overflow:hidden; */height:400px;margin-top:20px;}
.yhflashpurchase .flashpurchasebanner{position:relative;width:267px;height:100%; background:url(../images/03-brands/1.jpg) no-repeat center/cover;}
.flashpurchasebanner  > a{position:absolute;top:0px;right:0px;display:block;width:100%;height:100%;}
.flashpurchasebanner .flashtitle{position:absolute;z-index:1;left:50%;bottom:35px;margin-left:-100px;width:200px;height:100px;
background:rgba(255,255,255,.5);text-align:center;}
.flashpurchasebanner .flashtitle h2,h3{font-weight:normal;color:#000;}
.flashpurchasebanner .flashtitle h2{height:33px;padding-top:20px;}
.flashpurchasebanner .flashtitle h2:after{content:'';position:absolute;left:50%;top:50%;margin-left:-15px;margin-top:5px;height:11px;width:30px;border-top:1px solid #000;}
.flashpurchasebanner .flashtitle h3{margin-top:10px;}
.flashpurchasebanner .flashtitle:hover{background:rgba(255,255,255,.8);}
/* 
一号闪购之团购 */
.yhflashpurchase .grouppurchase{width:484px;height:100%;background:#fff;margin:0px 10px;border-top:1px solid #FB6233;}
.grouppurchasenav{position:relative;height:33px;margin-bottom:5px;background:url(../images/03-brands/6.png)no-repeat 0px 5px/cover;font-weight:normal;}
.grouppurchasenav > a{position:absolute;top:-5px;left:50%;margin-left:-70px;z-index:2;
 width:140px;height:32px;background-color:#FB6233;font-size:18px;color:#fff;text-align:center;line-height:32px;}
.grouppurchasenav > a:after{content:'';position:absolute;top:0px;right:-5px;width:0px;height:0px;border:3px solid transparent;border-left-color:#FB6233;border-bottom-color:#FB6233;}
.grouppurchasenav > a:before{content:'';position:absolute;top:0px;left:-5px;width:0px;height:0px;border:3px solid transparent;border-right-color:#FB6233;border-bottom-color:#FB6233;}
.yhflashpurchase .grouppurchase .group_conleft{width:250px;height:360px;
background:url(../images/03-brands/2.jpg)no-repeat center/cover;border-right:1px solid #eee;}
.yhflashpurchase /* .grouppurchase */ .group_conright{overflow:hidden;/* height:360px; */margin:0px 15px 0px 20px;}
.yhflashpurchase /* .grouppurchase */ .group_conright div{position:relative;width:198px;height:180px;padding-top:15px;font-weight:normal;border-bottom:1px solid #eee;}
.group_conright div h2,.group_conright div h3{font-weight:normal;line-height:30px;width:90px;}
.yhflashpurchase /* .grouppurchase  */.group_conright .salegroup img{position:absolute;right:0px;bottom:10px;width:110px;height:110px;transition:0.3s;}
.yhflashpurchase /* .grouppurchase */ .group_conright .selectgroup img{position:absolute;right:0px;bottom:40px;width:110px;height:110px;transition:0.3s;}
.yhflashpurchase /* .grouppurchase */ .group_conright .salegroup:hover img{right:5px;}
.yhflashpurchase /* .grouppurchase */ .group_conright .selectgroup:hover img{right:5px;}
