/* 酒水饮料特殊样式设计 */
.yhfresh  .freshdrink th{background:-webkit-linear-gradient(left,#96374F,#963759);}
.yhfresh  .freshdrink .bigfresh li{background:url(../images/08-fresh/8.jpg) no-repeat 15px 0px/cover;}
.yhfresh  .freshdrink td.drinktag{padding:10px 10px 10px 0px;width:175px;height:170px;}
.yhfresh  .freshdrink td.drinktag td{width:calc(168px / 2);height:calc(170px / 2);padding:0px; }
.yhfresh  .freshdrink td.drinktag td img{width:80px;height:32px;padding-top:24px;}

/* 
懂你想要样式整体设计 */
.knowyouwrap h2{margin-bottom:20px;text-align:center;font-weight:normal;}
.knowyouwrap h2 i{display:inline-block;width:27px;height:18px;background:url(../images/1.png) no-repeat -180px 0px;vertical-align:middle;}
.knowyouwrap h2 i + i{background:url(../images/1.png) no-repeat -220px 0px;}

.knowyouwrap li{position:relative;float:left;width:220px;height:300px;padding:8px;margin-left:9px;margin-bottom:10px;background:#fff;}
.knowyouwrap li img{width:220px;height:220px;}
.knowyouwrap li p{overflow:hidden;width:100%;height:40px;padding:5px 0px;line-height:20px;}
.knowyouwrap li h4{text-align:center;color:red;padding:0px;}
.knowyouwrap li >a{position:absolute;top:0px;left:0px;width:100%;height:100%;}
.knowyouwrap li >a:hover{border:1px solid #FF4040;}
.knowyou-foot{position:relative;width:370px;height:30px;margin:auto;border-bottom:1px solid #ddd;}
.knowyou-foot span{position:absolute;left:50%;bottom:-10px;margin-left:-75px;width:150px;background:#F5F5F5;font-size:18px;text-align:center;line-height:22px;}


/* 左边固定导航样式设计 */
.leftsidenav{position:fixed;left:50%;top:50%;margin-left:-660px;;margin-top:-220px;width:36px;}
.leftsidenav a{display:block;width:26px;height:36px;padding:5px;margin-bottom:3px;background-color:#fff;text-align:center;line-height:20px;}
.leftsidenav a:hover{color:#fff;background-color:#FE8282;}
.leftsidenav a:nth-child(1):hover{background-color:#645144;}
.leftsidenav a:nth-child(2):hover{background-color:#F78B4B;}
.leftsidenav a:nth-child(3):hover{background-color:#0BCD96;}
.leftsidenav a:nth-child(4):hover{background-color:#FF87D0;}

/* 右边固定导航样式设计 */

.rightsidenav{ position:fixed;top:0px;right:0px; height:100%;width:34px;background-color:#303030;}
.rightsidenav a{position:relative;display:block;width:34px;height:32px;margin:5px 0px;color:#fff;line-height:32px;font-weight:bold;text-align:center;}
.rightsidenav a i{font-size:18px;}
.rightsidenav a span.bartag{position:absolute;z-index:-1;top:0px;left:34px;height:32px;padding:0px 15px 0px 10px;
                          word-break:keep-all;font-weight:normal;background:#FF5C4D;line-height:32px;}
.rightsidenav .bartop a:nth-child(1){height:160px;background:url(../images/sidebar/1.gif) no-repeat center/cover;}
.rightsidenav .bartop a:nth-child(3){height:85px;width:18px;padding:10px 8px;}
.rightsidenav .bartop a:nth-child(3) p{line-height:18px;}
.rightsidenav .bartop a:nth-child(3) span{position:absolute;left:0px;top:0px;width:100%;height:100%;background-color:rgba(255,255,255,.2);}
.rightsidenav .barend{position:absolute;bottom:0px;left:0px;}
.rightsidenav .barend a:nth-child(2) span{display:none;position:absolute;z-index:-1;bottom:-25px;left:-140px;width:130px;height:140px;box-shadow:0px 0px 5px 1px #aaa;}
.rightsidenav .barend a:nth-child(2) span:after{content:'';position:absolute;bottom:38px;right:-5px;width:0px;height:0px;border:5px solid transparent;border-left-color:#fff; border-bottom-color:#fff;transform:rotate(-135deg);}
.rightsidenav a:hover{background-color:#FF5C4D;}
.rightsidenav a:hover span.bartag{left:-60px;transition:left 0.5s;}
.rightsidenav .barend a:nth-child(2):hover span{display:block; }