/* 全球进口 ***************************************************************************/

.globalimport{margin-top:20px;}
.globalimport dt{position:relative;}
.globalimport dt h2{height:47px;line-height:47px;text-indent:1em;color:#675548;}
.globalimport dt span{position:absolute;top:50%;left:100px;margin-top:-8px;height:17px;
background-color:#675548;border-radius:50%;line-height:17px;}
.globalimport .importbanner{position:relative;width:316px;height:450px;
background:url(../images/06-global/1.jpg)no-repeat center/cover;}
.globalimport  .importbanner div{ position:absolute;width:285px;left:50%;bottom:15px; margin-left:-142.5px;}
.globalimport  .importbanner a{float:left;width:85px;height:30px;margin:5px;color:#fff;line-height:30px;
text-align:center;font-size:14px;background:rgba(255,255,255,.1);}
.globalimport  .importbanner a:hover{background:rgba(255,255,255,.5);}
/* 表格样式的设计 */
.importcontent{width:604px; height:440px;background:#B96131;padding:5px 5px 5px 0px;}
.globalimport table{border-collapse:collapse;}
.globalimport table td{width:200px;height:220px;border:1px solid #675548;padding:0px;background:#fff;transition:transform 0.5s;}
.globalimport table td img{float:right;width:140px;height:140px;margin-right:10px;margin-bottom:10px;}
.globalimport table td h3{text-indent:1.5em;line-height:25px;}
.globalimport table td h3:nth-child(2){color:#ccc;}
.globalimport table .contentimg{background:url(../images/06-global/2.jpg)no-repeat center/cover;}
.globalimport table td:hover{transform:scale(1.1);}
/* 全球进口右边导航栏的设计 */
.globalimport .importright{width:300px;height:450px;}
.importright a{position:relative;float:left;width:150px;height:140px;padding-top:10px;}
.importright a span{display:block;width:20px;height:3px;margin:10px 0px 0px 14px;background:#F16E99;}
.importright a h3{text-indent:1em;line-height:26px;font-weight:lighter;color:#F16E99;}
.importright a p{color:#aaa;font-size:12px;text-indent:1.2em;font-weight:lighter;}
.importright a img{position:absolute;right:0px;bottom:0px;width:100px;height:100px;transition:0.5s;}
.importright a:hover img{right:15px;}

/* 
国产食品公共样式设计*************************************************************************** */
.madeincommon{height:430px;margin-top:20px;}
/* 最左边样式设计 */
.madeincommon .combanner{height:410px;width:236px;padding-top:20px;background:linear-gradient(#F89959,#F9AF62);
color:#fff;text-align:center;font-size:14px;}
.combanner > a:first-of-type{display:block;width:120px;height:32px;margin:auto;border-bottom:1px solid #fff;
text-align:center;line-height:32px;font-size:20px;vertical-align:middle;}
.combanner > a i{vertical-align:middle;}

/* 左边轮播图样式 */
.combanner .foodcard{height:225px;margin-top:35px;}/* 轮播图可视区设计 */

.combanner .foodcard ul{position:relative;width:195px;margin:auto;}/* 容纳轮播图片的大容器 */
/* 要轮播的单一项目样式设计 */
.combanner .foodcard .foodlist{position:absolute;top:0px;left:0px;width:195px;height:205px;
padding-top:20px;background:#fff;border-radius:5px;}
.combanner .foodcard .foodlist  img{width:130px;height:130px;}
.foodlist h3{line-height:20px;color:#F89959;}
.foodlist p{color:#aaa;text-align:center;line-height:30px; }
.combanner .foodcard .foodlist0{z-index:3;}
.combanner .foodcard .foodlist1{z-index:2;width:175px;top:-10px;left:50%;margin-left:-87.5px;}
.combanner .foodcard .foodlist2{z-index:1;width:155px;top:-20px;left:50%;margin-left:-77.5px;}
/* 为轮播的单个项目加一个遮罩层，这样在定位的时候可以覆盖后面定位上来的图片 */
.foodlist1 .colorbox,.foodlist2 .colorbox{position:absolute;top:0px;height:0px;height:100%;width:100%;
 background:#F78F50; opacity:0.6; }
.foodlist2 .colorbox{opacity:0.3;}
/* 轮播图下面的操作按钮样式设计 */
.combanner .foodbtn{height:30px;margin-top:10px;line-height:30px;word-spacing:10px;letter-spacing:5px;}
.combanner .foodnote{height:26px;width:100%;padding:8px 0px;;margin-top:15px;background:rgba(255,255,255,.5);}
.combanner .foodnote span{float:left;width:43px;height:26px;line-height:28px;border-right:1px solid #fff;text-align:center;}

